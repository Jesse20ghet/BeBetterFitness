<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jessemoreland.bebetterfitness">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <!--android:theme="@style/AppTheme">-->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.ExerciseActivity"></activity>
        <activity android:name=".Activities.ConfigurationActivity"></activity>
        <activity android:name=".Activities.NutritionActivity"></activity>
        <activity android:name=".Activities.RequestActivity"></activity>
        <activity android:name=".Activities.CreateALiftActivity"></activity>
        <activity android:name=".Activities.CreateAWorkoutActivity"></activity>
        <activity android:name=".Activities.DebugActivity"></activity>
        <activity android:name=".Activities.EditAWorkoutActivity"></activity>
        <activity android:name=".Activities.EditALiftActivity"></activity>
        <activity android:name=".Activities.EditIntermediateActivity"></activity>
        <activity android:name=".Activities.PerformWorkoutActivity"></activity>
        <activity android:name=".Activities.ChooseAWorkoutIntermediateActivity"></activity>

    </application>

</manifest>
